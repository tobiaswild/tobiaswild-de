{
  "public\\sitemap.xml": {
    "Protocol": "https",
    "DomainName": "tobiaswild.de",
    "Root": "./pages",
    "IncludeExt": [".tsx"],
    "Exclude": ["_app.tsx", "_document.tsx", "500.tsx", "404.tsx", "_error.tsx", "blog/[slug].tsx"],
    "TagsToInclude": ["priority", "lastmod"],
    "DefaultChangeFrequency": "monthly",
    "TabCharacters": "\t",
    "bIncludeWWW": true,
    "bRemoveFileExtentions": true,
    "bUseTrailingSlash": false,
    "bAutomaticallyUpdateSitemap": true,
    "bMinimized": false
  }
}
